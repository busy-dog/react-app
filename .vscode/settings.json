{
  "files.eol": "\n",
  "eslint.enable": true,
  "editor.tabSize": 2,
  "editor.indentSize": "tabSize",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "eslint.autoFixOnSave": "explicit"
  },
  "css.format.enable": true,
  "css.format.preserveNewLines": true,
  "css.format.newlineBetweenRules": true,
  "css.format.spaceAroundSelectorSeparator": false,
  "scss.format.enable": true,
  "scss.format.preserveNewLines": true,
  "scss.format.newlineBetweenRules": true,
  "scss.format.spaceAroundSelectorSeparator": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.DS_Store": true,
  },
  "files.associations": {
    "*.env": "ini",
    "assets/index.html": "ejs"
  },
  "cssVariables.lookupFiles": [
    "**/themes/*.css",
  ],
  "css-module-typed.pluginOptions": {
    "goToDefinition": true,
    "classnameTransform": "camelCaseOnly",
    "customMatcher": "\\.(c|le|sa|sc)ss$",
    "allowUnknownClassnamesAsDeprecated": true
  },
  "[svg]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "[json]": {
    "editor.formatOnSave": true,
    "editor.quickSuggestions": {
      "other": "on",
      "comments": "off",
      "strings": true
    },
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[css]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[scss]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "git.ignoreLimitWarning": true,
  "i18n-ally.localesPaths": [
    "src/i18n"
  ],
  "i18n-ally.namespace": true,
  "i18n-ally.keystyle": "nested",
  "i18n-ally.displayLanguage": "zh-CN",
}